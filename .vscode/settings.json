{
    "cSpell.words": [
        "beamer",
        "BÚNO",
        "DIMACS",
        "Hilbertova",
        "holubníkového",
        "konst",
        "kvantifikátorový",
        "numerál",
        "podformulemi",
        "prenexním",
        "řešič",
        "satisfiability",
        "solvingu",
        "SÚNO"
    ],
    "cSpell.ignoreWords": [
        "Fabio",
        "Fraenkel",
        "NTIN",
        "Presburger",
        "Szmielew",
        "Tarski",
        "Unif",
        "Zermelo",
        "disj",
        "jedn",
        "nejob",
        "zákl"
    ],
    "latex-workshop.latex.clean.enabled": true,
    "latex-workshop.latex.clean.fileTypes": [
        ".aux",
        ".log",
        ".fls",
        ".fdb_latexmk",
        ".synctex.gz"
    ],
    "latex-workshop.latex.recipes": [
        {
            "name": "pdflatex with cleanup",
            "tools": [
                "pdflatex",
                "cleanup"
            ]
        }
    ],
    "latex-workshop.latex.tools": [
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "-interaction=nonstopmode",
                "-synctex=1",
                "%DOC%"
            ]
        },
        {
            "name": "cleanup",
            "command": "bash",
            "args": [
                "-c",
                "rm -f *.aux *.log *.fls *.fdb_latexmk *.synctex.gz"
            ]
        }
    ],
    "latex-workshop.latex.recipe.default": "pdflatex with cleanup"
}