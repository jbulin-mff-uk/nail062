{
    "cSpell.words": [
        "beamer",
        "BÚNO",
        "DIMACS",
        "Hilbertova",
        "holubníkového",
        "konst",
        "kvantifikátorový",
        "numerál",
        "podformulemi",
        "prenexním",
        "řešič",
        "satisfiability",
        "solvingu",
        "SÚNO"
    ],
    "cSpell.ignoreWords": [
        "Fabio",
        "Fraenkel",
        "NTIN",
        "Presburger",
        "Szmielew",
        "Tarski",
        "Unif",
        "Zermelo",
        "disj",
        "jedn",
        "nejob",
        "zákl"
    ],
    "latex-workshop.latex.clean.enabled": true,
    "latex-workshop.latex.clean.fileTypes": [
        ".aux",
        ".log",
        ".fls",
        ".fdb_latexmk",
        ".synctex.gz"
    ],
    "latex-workshop.latex.recipes": [
            {
                "name": "pdflatex with cleanup",
                "tools": [
                    "pdflatex",
                    "cleanup"
                ]
            },
            {
                "name": "pdflatex x2 + clean",
                "tools": ["pdflatex", "pdflatex", "clean"]
            }
    ],
    "latex-workshop.latex.tools": [
            {
                "name": "pdflatex",
                "command": "pdflatex",
                "args": [
                    "-interaction=nonstopmode",
                    "-file-line-error",
                    "%DOCFILE%"
                ]
            },
            {
                "name": "cleanup",
                "command": "bash",
                "args": [
                    "-c",
                    "rm -f *.aux *.log *.fls *.fdb_latexmk *.synctex.gz"
                ]
            },
            {
                "name": "clean",
                "command": "latexmk",
                "args": [
                    "-c"
                ]
            }
    ],
    "latex-workshop.latex.recipe.default": "pdflatex with cleanup"
}